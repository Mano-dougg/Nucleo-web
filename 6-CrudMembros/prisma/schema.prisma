generator client {
  provider = "prisma-client-js"
}y

datasource db {
  provider = "mysql"
  url      = env("mysql://localhost:3306/crudemembros")
}


model Membro {
  id Int @id @default(autoincrement())
  name String
  password String
  birth DateTime
  state String
  city String
}
